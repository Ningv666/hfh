ure=\"checkRepwd\"></mt-field>\r\n    <mt-button type=\"primary\" size=\"large\" @click=\"checkForm\">免费注册</mt-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data(){\r\n    return {\r\n      uname:'',\r\n      upwd:'',\r\n      repwd:'',\r\n      nameState:'',\r\n      upwdState:'',\r\n      repwdState:''\r\n    }\r\n  },\r\n  methods:{\r\n    checkName(){//验证用户名\r\n      let reg = /^\\w{6,15}$/;\r\n      if(reg.test(this.uname)){\r\n        this.nameState = 'success';\r\n        return true;\r\n      }else {\r\n        this.nameState = 'error';\r\n        return false;\r\n      }\r\n    },\r\n        checkUpwd(){//验证密码\r\n      let reg = /^\\d{6}$/;\r\n      if(reg.test(this.upwd)){\r\n        this.upwdState = 'success';\r\n        return true;\r\n      }else {\r\n        this.upwdState = 'error';\r\n        return false;\r\n      }\r\n    },\r\n        checkRepwd(){//验证确认密码\r\n        let reg = /^\\d{6}$/;\r\n      if(this.upwd==this.repwd&&this.repwd.match(reg)){\r\n        this.repwdState = 'success';\r\n        return true;\r\n      }else {\r\n        this.repwdState = 'error';\r\n        return false;\r\n      }\r\n    },\r\n    checkForm(){//验证表单\r\n      if(this.checkName() && this.checkUpwd() && this.checkRepwd())\r\n      {\r\n        console.log(`发请求，执行注册业务`);\r\n        this.axios.post('/register',`username=${this.uname}&password=${this.upwd}`).then(res=>{\r\n          console.log(res);\r\n          if(res.code==200){\r\n            // 注册成功\r\n            this.$toast({\r\n              message:'注册成功',\r\n              position:'bottom',\r\n              duration:3000\r\n            })\r\n            this.$router.push('/login')\r\n          }else if(res.data.code==201){//用户已存在\r\n            this.$toast({\r\n              message:'注册失败，用户已存在',\r\n              position:'bottom',\r\n              duration:3000\r\n            })\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>"]}]}