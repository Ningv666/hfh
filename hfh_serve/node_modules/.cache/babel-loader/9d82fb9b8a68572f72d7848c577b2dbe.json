\">注册</router-link>\r\n    </mt-header>\r\n    <!-- 表单组件 -->\r\n    <mt-field type=\"text\" placeholder=\"请输入用户名\" label=\"用户名\" v-model=\"uname\" :state=\"nameState\" @blur.native.capture=\"checkName\"></mt-field>\r\n    <mt-field type=\"password\" placeholder=\"请输入密码\" label=\"密码\" v-model=\"upwd\" :state=\"upwdState\" @blur.native.capture=\"checkUpwd\"></mt-field>\r\n    <mt-button type=\"primary\" size=\"large\" @click=\"checkForm\">登录</mt-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data(){\r\n    return {\r\n      uname:'',\r\n      upwd:'',\r\n      nameState:'',\r\n      upwdState:''\r\n    }\r\n  },\r\n  methods:{\r\n    checkName(){//验证用户名\r\n      let reg = /^\\w{6,15}$/;\r\n      if(reg.test(this.uname)){\r\n        this.nameState = 'success';\r\n        return true;\r\n      }else {\r\n        this.nameState = 'error';\r\n        return false;\r\n      }\r\n    },\r\n        checkUpwd(){//验证密码\r\n      let reg = /^\\d{6}$/;\r\n      if(reg.test(this.upwd)){\r\n        this.upwdState = 'success';\r\n        return true;\r\n      }else {\r\n        this.upwdState = 'error';\r\n        return false;\r\n      }\r\n    },\r\n    checkForm(){//验证表单\r\n      if(this.checkName() && this.checkUpwd())\r\n      {\r\n        console.log(`发请求，执行登录业务`);\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>"],"sourceRoot":"src/views"}]}