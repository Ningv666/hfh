A+BA,IAAA,SA/BA,uBA+BA;AAAA;;AAAA;AACA,UAAA,KAAA,SAAA,MAAA,KAAA,SAAA,EAAA,IAAA,KAAA,UAAA,EAAA,EACA;AACA,QAAA,OAAA,CAAA,GAAA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,WAAA,qBAAA,KAAA,KAAA,uBAAA,KAAA,IAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,cAAA,GAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA;AACA,YAAA,KAAA,CAAA,MAAA,CAAA;AACA,cAAA,OAAA,EAAA,MADA;AAEA,cAAA,QAAA,EAAA,QAFA;AAGA,cAAA,QAAA,EAAA;AAHA,aAAA;;AAKA,YAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,WARA,MAQA,IAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AAAA;AACA,YAAA,KAAA,CAAA,MAAA,CAAA;AACA,cAAA,OAAA,EAAA,YADA;AAEA,cAAA,QAAA,EAAA,QAFA;AAGA,cAAA,QAAA,EAAA;AAHA,aAAA;AAKA;AACA,SAjBA;AAkBA;AACA;AAtDA;AAXA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <mt-header title=\"注册\">\r\n      <mt-button icon='back' slot=\"left\"></mt-button>\r\n      <router-link to=\"/login\" slot=\"right\">登录</router-link>\r\n    </mt-header>\r\n    <!-- 表单组件 -->\r\n    <mt-field type=\"text\" placeholder=\"请输入用户名\" label=\"用户名\" v-model=\"uname\" :state=\"nameState\" @blur.native.capture=\"checkName\"></mt-field>\r\n    <mt-field type=\"password\" placeholder=\"密码由6位数字组成\" label=\"密码\" v-model=\"upwd\" :state=\"upwdState\" @blur.native.capture=\"checkUpwd\"></mt-field>\r\n    <mt-field type=\"password\" placeholder=\"请再次输入密码\" label=\"确认密码\" v-model=\"repwd\" :state=\"repwdState\" @blur.native.capture=\"checkRepwd\"></mt-field>\r\n    <mt-button type=\"primary\" size=\"large\" @click=\"checkForm\">免费注册</mt-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data(){\r\n    return {\r\n      uname:'',\r\n      upwd:'',\r\n      repwd:'',\r\n      nameState:'',\r\n      upwdState:'',\r\n      repwdState:''\r\n    }\r\n  },\r\n  methods:{\r\n    checkName(){//验证用户名\r\n      let reg = /^\\w{6,15}$/;\r\n      if(reg.test(this.uname)){\r\n        this.nameState = 'success';\r\n        return true;\r\n      }else {\r\n        this.nameState = 'error';\r\n        return false;\r\n      }\r\n    },\r\n        checkUpwd(){//验证密码\r\n      let reg = /^\\d{6}$/;\r\n      if(reg.test(this.upwd)){\r\n        this.upwdState = 'success';\r\n        return true;\r\n      }else {\r\n        this.upwdState = 'error';\r\n        return false;\r\n      }\r\n    },\r\n        checkRepwd(){//验证确认密码\r\n        let reg = /^\\d{6}$/;\r\n      if(this.upwd==this.repwd&&this.repwd.match(reg)){\r\n        this.repwdState = 'success';\r\n        return true;\r\n      }else {\r\n        this.repwdState = 'error';\r\n        return false;\r\n      }\r\n    },\r\n    checkForm(){//验证表单\r\n      if(this.checkName() && this.checkUpwd() && this.checkRepwd())\r\n      {\r\n        console.log(`发请求，执行注册业务`);\r\n        this.axios.post('/register',`username=${this.uname}&password=${this.upwd}`).then(res=>{\r\n          console.log(res);\r\n          if(res.code==200){\r\n            // 注册成功\r\n            this.$toast({\r\n              message:'注册成功',\r\n              position:'bottom',\r\n              duration:3000\r\n            })\r\n            this.$router.push('/login')\r\n          }else if(res.data.code==201){//用户已存在\r\n            this.$toast({\r\n              message:'注册失败，用户已存在',\r\n              position:'bottom',\r\n              duration:3000\r\n            })\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>"],"sourceRoot":"src/views"}]}