AAA,EAAA,MADA;AAEA,cAAA,QAAA,EAAA,QAFA;AAGA,cAAA,QAAA,EAAA;AAHA,aAAA;;AAKA,YAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,SAAA,EAAA,KAAA,CAAA,KAAA;;AACA,YAAA,cAAA,CAAA,OAAA,CAAA,SAAA,EAAA,IAAA;AACA,YAAA,cAAA,CAAA,OAAA,CAAA,MAAA,EAAA,KAAA,CAAA,KAAA;;AAGA,YAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,WAbA,MAaA,IAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AAAA;AACA,YAAA,KAAA,CAAA,MAAA,CAAA;AACA,cAAA,OAAA,EAAA,eADA;AAEA,cAAA,QAAA,EAAA,QAFA;AAGA,cAAA,QAAA,EAAA;AAHA,aAAA;AAKA;AACA,SAtBA;AAuBA;AACA;AAhDA;AATA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <mt-header title=\"登录\">\r\n      <mt-button icon='back' slot=\"left\" ></mt-button>\r\n      <router-link to=\"/register\" slot=\"right\">注册</router-link>\r\n    </mt-header>\r\n    <!-- 表单组件 -->\r\n    <mt-field type=\"text\" placeholder=\"请输入用户名\" label=\"用户名\" v-model=\"uname\" :state=\"nameState\" @blur.native.capture=\"checkName\"></mt-field>\r\n    <mt-field type=\"password\" placeholder=\"请输入密码\" label=\"密码\" v-model=\"upwd\" :state=\"upwdState\" @blur.native.capture=\"checkUpwd\"></mt-field>\r\n    <mt-button type=\"primary\" size=\"large\" @click=\"checkForm\">登录</mt-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data(){\r\n    return {\r\n      uname:'',\r\n      upwd:'',\r\n      nameState:'',\r\n      upwdState:''\r\n    }\r\n  },\r\n  methods:{\r\n    checkName(){//验证用户名\r\n      let reg = /^\\w{6,15}$/;\r\n      if(reg.test(this.uname)){\r\n        this.nameState = 'success';\r\n        return true;\r\n      }else {\r\n        this.nameState = 'error';\r\n        return false;\r\n      }\r\n    },\r\n        checkUpwd(){//验证密码\r\n      let reg = /^\\d{6}$/;\r\n      if(reg.test(this.upwd)){\r\n        this.upwdState = 'success';\r\n        return true;\r\n      }else {\r\n        this.upwdState = 'error';\r\n        return false;\r\n      }\r\n    },\r\n    checkForm(){//验证表单\r\n      if(this.checkName() && this.checkUpwd())\r\n      {\r\n        this.axios.post('/login',`username=${this.uname}&password=${this.upwd}`).then(res=>{\r\n          console.log(res);\r\n          if(res.data.code==200){\r\n            // 注册成功\r\n            this.$toast({\r\n              message:'登录成功',\r\n              position:'bottom',\r\n              duration:3000\r\n            })\r\n            this.$store.commit('loginOK',this.uname);\r\n            sessionStorage.setItem('islogin',true);\r\n            sessionStorage.setItem('name',this.uname);\r\n\r\n\r\n            this.$router.push('/')\r\n          }else if(res.data.code==201){//用户已存在\r\n            this.$toast({\r\n              message:'登录失败，用户名或密码错误',\r\n              position:'bottom',\r\n              duration:3000\r\n            })\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>"],"sourceRoot":"src/views"}]}